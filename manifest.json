{
  "manifest_version": 2,
  "name": "messydesk",
  "version": "1.0",
  "description": "A todo list that isn't a todo list",
  "author": "malbert",
  "chrome_url_overrides" : {
    "newtab": "extension/build/index.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}